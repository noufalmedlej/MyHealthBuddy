<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_margin="10dp"
    app:cardCornerRadius="8dp"
    app:cardElevation="10dp">



    <RelativeLayout
        android:layout_width="370dp"
        android:layout_height="116dp"
        android:layout_margin="5dp"
        android:orientation="vertical">


        <TextView
            android:id="@+id/all_HCP_profile_name"
            android:layout_width="208dp"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_gravity="right"
            android:layout_marginStart="25dp"
            android:layout_marginLeft="25dp"
            android:layout_marginTop="7dp"
            android:layout_marginEnd="137dp"
            android:layout_marginRight="137dp"
            android:text="اسم مقدم الرعاية الصحية"
            android:textSize="16dp"
            android:gravity="end"
            android:textAlignment="viewEnd"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/all_HCP_profile_specialty"
            android:layout_width="199dp"
            android:layout_height="22dp"
            android:layout_alignStart="@+id/all_HCP_profile_name"
            android:layout_alignLeft="@+id/all_HCP_profile_name"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentBottom="true"
            android:layout_gravity="right"
            android:layout_marginStart="1dp"
            android:layout_marginLeft="1dp"
            android:layout_marginTop="30dp"
            android:layout_marginEnd="162dp"
            android:layout_marginRight="162dp"
            android:layout_marginBottom="64dp"
            android:text="التحصص "
            android:textSize="16dp"
            android:gravity="end"
            android:textAlignment="viewEnd"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/all_HCP_profile_Id"
            android:layout_width="200dp"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_gravity="right"
            android:layout_marginStart="51dp"
            android:layout_marginLeft="51dp"
            android:layout_marginTop="53dp"
            android:layout_marginEnd="141dp"
            android:layout_marginRight="141dp"
            android:gravity="end"
            android:text="رقم مقدم الرعاية"
            android:textAlignment="viewEnd"
            android:textSize="16dp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/all_HCP_profile_gender"
            android:layout_width="115dp"
            android:layout_height="wrap_content"
            android:layout_alignStart="@+id/all_HCP_profile_Id"
            android:layout_alignLeft="@+id/all_HCP_profile_Id"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_gravity="right"
            android:layout_marginStart="29dp"
            android:layout_marginLeft="29dp"
            android:layout_marginTop="78dp"
            android:layout_marginEnd="160dp"
            android:layout_marginRight="160dp"
            android:text="الجنس"
            android:textSize="16dp"
            android:textStyle="bold"
            android:gravity="end"
            android:textAlignment="viewEnd"/>

        <de.hdodenhof.circleimageview.CircleImageView
            android:id="@+id/all_HCP_profileImg"
            android:layout_width="69dp"
            android:layout_height="78dp"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentBottom="true"
            android:layout_marginLeft="10dp"
            android:layout_marginEnd="8dp"
            android:layout_marginRight="8dp"
            android:layout_marginBottom="34dp"
            android:src="@drawable/docmale" />

        <TextView
            android:id="@+id/textView2"
            android:layout_width="wrap_content"
            android:layout_height="22dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="236dp"
            android:layout_marginLeft="236dp"
            android:layout_marginTop="6dp"
            android:layout_marginBottom="88dp"
            android:text="الاسم :"
            android:gravity="end"
            android:textAlignment="viewEnd"
            android:textColor="#131010"
            android:textSize="18sp" />

        <TextView
            android:id="@+id/textView3"
            android:layout_width="76dp"
            android:layout_height="21dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="204dp"
            android:layout_marginLeft="204dp"
            android:layout_marginTop="30dp"
            android:layout_marginEnd="90dp"
            android:layout_marginRight="90dp"
            android:layout_marginBottom="65dp"
            android:lineSpacingExtra="18sp"
            android:text="التخصص :"
            android:textColor="#0C0A0A"
            android:textSize="18sp" />

        <TextView
            android:id="@+id/textView4"
            android:layout_width="wrap_content"
            android:layout_height="22dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="232dp"
            android:layout_marginLeft="232dp"
            android:layout_marginTop="52dp"
            android:layout_marginEnd="90dp"
            android:layout_marginRight="90dp"
            android:layout_marginBottom="42dp"
            android:text="الرقم:"
            android:textColor="#050101"
            android:textSize="18sp" />

        <TextView
            android:id="@+id/textView5"
            android:layout_width="68dp"
            android:layout_height="21dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="213dp"
            android:layout_marginLeft="213dp"
            android:layout_marginTop="78dp"
            android:layout_marginEnd="89dp"
            android:layout_marginRight="89dp"
            android:layout_marginBottom="17dp"
            android:text="الجنس: "
            android:textColor="#0C0505"
            android:textSize="18sp" />


    </RelativeLayout>


</androidx.cardview.widget.CardView>